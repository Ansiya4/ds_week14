  {
   "cell_type": "code",
   "execution_count": 327,
   "id": "85a7197b-116e-4c3e-bdb8-e85792aa3f54",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Step 2: Prepare Data\n",
    "x = tf.constant([1, 2, 3, 4], dtype=tf.float32)\n",
    "y = tf.constant([2, 4, 6, 8], dtype=tf.float32)\n",
    "shape=(1,)"
   ]
  },
  {adsda
   "cell_type": "code",
